<div class="modal-header">
    <h3 class="modal-title">查看操作日志</h3>
</div>
<form name="detailForm" action="" class="horizontal-form">
    <div class="modal-body">
        <div class="form-body">
            <div class="form-group">
                <label class="control-label">级别</label>
                {{Logs.LogLevel}}
            </div>
            <div class="form-group">
                <label class="control-label">时间</label>
                {{Logs.LogTime|date:"yyyy-MM-dd HH:mm:ss"}}
            </div>
            <div class="form-group">
                <label class="control-label">操作文本</label>
                {{Logs.Message}}
            </div>
        </div>
    </div>

    <div class="modal-footer">
        <button class="btn btn-warning" ng-click="cancel()">关闭</button>
    </div>
</form>

