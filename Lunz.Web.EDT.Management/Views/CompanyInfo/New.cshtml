                             data-ng-class="{'has-error': editForm.ContactPhone.$dirty && editForm.ContactPhone.$invalid}">
                            <label class="control-label">手机号码 <span class="required">*</span></label>
                            <input type="text" name="ContactPhone" data-ng-model="CompanyInfo.ContactPhone"
                                   required maxlength="50" class="form-control" ng-pattern="/(^0?[1][358][0-9]{9}$)/" />
                            <span class="help-block"
                                  data-ng-show="editForm.ContactPhone.$dirty && editForm.ContactPhone.$invalid">
                                此字段为必填或格式不对。
                            </span>
                        </div>
                        <div class="form-group"
                             data-ng-class="{'has-error': editForm.ContactEmail.$dirty && editForm.ContactEmail.$invalid}">
                            <label class="control-label">邮箱 <span class="required">*</span></label>

                            <div class="input-group">
                                <span class="input-group-addon">
                                    <i class="fa fa-envelope"></i>
                                </span>
                                <input type="email" name="ContactEmail" data-ng-model="CompanyInfo.ContactEmail"
                                       required maxlength="50" class="form-control" />
                            </div>
                            <span class="help-block"
                                  data-ng-show="editForm.ContactEmail.$dirty && editForm.ContactEmail.$invalid">
                                此字段为必填或格式不对。
